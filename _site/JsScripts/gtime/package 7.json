{
  "name": "d3-gtimeline",
  "version": "1.0.1",
  "description": "Google-like timeline chart",
  "keywords": [
    "d3",
    "d3-module",
    "timeline",
    "d3-chart"
  ],
  "type": "module",
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "src/**/*.js"
  ],
  "main": "dist/d3-gtimeline.umd.cjs",
  "module": "dist/d3-gtimeline.js",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/jjagielka/d3-gtimeline",
  "exports": {
    ".": {
      "umd": "./dist/d3-gtimeline.umd.cjs",
      "default": "./src/index.js",
      "import": "./dist/d3-gtimeline.js",
      "require": "./dist/d3-gtimeline.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjagielka/d3-gtimeline.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite -c vite.config.full.js build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "d3-array": "3",
    "d3-axis": "3",
    "d3-color": "3",
    "d3-drag": "3",
    "d3-scale": "4",
    "d3-selection": "3",
    "d3-time": "3",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "dayjs": "^1.11.10"
  }
}
